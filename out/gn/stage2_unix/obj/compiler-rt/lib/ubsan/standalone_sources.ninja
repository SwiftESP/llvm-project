build stage2_unix/obj/compiler-rt/lib/ubsan/standalone_sources.stamp: stage2_unix_stamp stage2_unix/obj/compiler-rt/lib/ubsan/standalone_sources.ubsan_diag_standalone.o stage2_unix/obj/compiler-rt/lib/ubsan/standalone_sources.ubsan_init_standalone.o stage2_unix/obj/compiler-rt/lib/ubsan/standalone_sources.ubsan_signals_standalone.o
