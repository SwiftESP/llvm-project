build stage2_unix/obj/compiler-rt/lib/ubsan/standalone_sources.ubsan_signals_standalone.o: stage2_unix_cxx ../../compiler-rt/lib/ubsan/ubsan_signals_standalone.cpp || stage2_unix/obj/compiler-rt/lib/ubsan/standalone_sources.inputdeps.stamp
  source_file_part = ubsan_signals_standalone.cpp
