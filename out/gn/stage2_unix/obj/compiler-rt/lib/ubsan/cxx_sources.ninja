defines = -DUBSAN_CAN_USE_CXXABI
include_dirs = -I../../compiler-rt/lib
cflags = -mmacos-version-min=10.10 -O3 -fdiagnostics-color -Wall -Wextra -Wno-unused-parameter -Wdelete-non-virtual-dtor -Wstring-conversion -no-canonical-prefixes -Werror=date-time -isysroot ../../../../../../Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk -Wcovered-switch-default -fno-builtin -gline-tables-only -fPIC -funwind-tables -fvisibility=hidden -Werror=thread-safety -Werror=thread-safety-reference -Werror=thread-safety-beta
cflags_cc = -std=c++17 -fvisibility-inlines-hidden -fno-exceptions
label_name = cxx_sources
target_output_name = cxx_sources

build stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.inputdeps.stamp: stage2_unix_stamp obj/clang.stamp obj/lld.stamp
build stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.ubsan_handlers_cxx.o: stage2_unix_cxx ../../compiler-rt/lib/ubsan/ubsan_handlers_cxx.cpp || stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.inputdeps.stamp
  source_file_part = ubsan_handlers_cxx.cpp
  source_name_part = ubsan_handlers_cxx
build stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.ubsan_type_hash.o: stage2_unix_cxx ../../compiler-rt/lib/ubsan/ubsan_type_hash.cpp || stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.inputdeps.stamp
  source_file_part = ubsan_type_hash.cpp
  source_name_part = ubsan_type_hash
build stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.ubsan_type_hash_itanium.o: stage2_unix_cxx ../../compiler-rt/lib/ubsan/ubsan_type_hash_itanium.cpp || stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.inputdeps.stamp
  source_file_part = ubsan_type_hash_itanium.cpp
  source_name_part = ubsan_type_hash_itanium
build stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.ubsan_type_hash_win.o: stage2_unix_cxx ../../compiler-rt/lib/ubsan/ubsan_type_hash_win.cpp || stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.inputdeps.stamp
  source_file_part = ubsan_type_hash_win.cpp
  source_name_part = ubsan_type_hash_win

build stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.stamp: stage2_unix_stamp stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.ubsan_handlers_cxx.o stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.ubsan_type_hash.o stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.ubsan_type_hash_itanium.o stage2_unix/obj/compiler-rt/lib/ubsan/cxx_sources.ubsan_type_hash_win.o
