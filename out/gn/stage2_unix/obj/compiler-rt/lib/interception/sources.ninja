defines =
include_dirs = -I../../compiler-rt/lib
cflags = -mmacos-version-min=10.10 -O3 -fdiagnostics-color -Wall -Wextra -Wno-unused-parameter -Wdelete-non-virtual-dtor -Wstring-conversion -no-canonical-prefixes -Werror=date-time -isysroot ../../../../../../Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk -Wcovered-switch-default -fno-builtin -gline-tables-only -fPIC -funwind-tables -fvisibility=hidden -Werror=thread-safety -Werror=thread-safety-reference -Werror=thread-safety-beta
cflags_cc = -std=c++17 -fvisibility-inlines-hidden -fno-exceptions -fno-rtti
label_name = sources
target_output_name = sources

build stage2_unix/obj/compiler-rt/lib/interception/sources.inputdeps.stamp: stage2_unix_stamp obj/clang.stamp obj/lld.stamp
build stage2_unix/obj/compiler-rt/lib/interception/sources.interception_linux.o: stage2_unix_cxx ../../compiler-rt/lib/interception/interception_linux.cpp || stage2_unix/obj/compiler-rt/lib/interception/sources.inputdeps.stamp
  source_file_part = interception_linux.cpp
  source_name_part = interception_linux
build stage2_unix/obj/compiler-rt/lib/interception/sources.interception_mac.o: stage2_unix_cxx ../../compiler-rt/lib/interception/interception_mac.cpp || stage2_unix/obj/compiler-rt/lib/interception/sources.inputdeps.stamp
  source_file_part = interception_mac.cpp
  source_name_part = interception_mac
build stage2_unix/obj/compiler-rt/lib/interception/sources.interception_type_test.o: stage2_unix_cxx ../../compiler-rt/lib/interception/interception_type_test.cpp || stage2_unix/obj/compiler-rt/lib/interception/sources.inputdeps.stamp
  source_file_part = interception_type_test.cpp
  source_name_part = interception_type_test
build stage2_unix/obj/compiler-rt/lib/interception/sources.interception_win.o: stage2_unix_cxx ../../compiler-rt/lib/interception/interception_win.cpp || stage2_unix/obj/compiler-rt/lib/interception/sources.inputdeps.stamp
  source_file_part = interception_win.cpp
  source_name_part = interception_win

build stage2_unix/obj/compiler-rt/lib/interception/sources.stamp: stage2_unix_stamp stage2_unix/obj/compiler-rt/lib/interception/sources.interception_linux.o stage2_unix/obj/compiler-rt/lib/interception/sources.interception_mac.o stage2_unix/obj/compiler-rt/lib/interception/sources.interception_type_test.o stage2_unix/obj/compiler-rt/lib/interception/sources.interception_win.o
