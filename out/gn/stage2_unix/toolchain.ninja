rule stage2_unix_copy
  command = ln -f ${in} ${out} 2>/dev/null || (rm -rf ${out} && cp -af ${in} ${out})
  description = COPY ${in} ${out}
rule stage2_unix_objcxx
  command = ./bin/clang++ -MMD -MF ${out}.d -o ${out} -c ${in} ${defines} ${include_dirs} ${cflags} ${cflags_objcc}
  description = OBJCXX ${out}
  depfile = ${out}.d
  deps = gcc
rule stage2_unix_asm
  command = ./bin/clang -MMD -MF ${out}.d -o ${out} -c ${in} ${defines} ${include_dirs} ${asmflags}
  description = ASM ${out}
  depfile = ${out}.d
  deps = gcc
rule stage2_unix_alink
  command = libtool -D -static -no_warning_for_no_symbols ${arflags} -o ${out} ${in}
  description = AR ${out}
rule stage2_unix_solink
  command = ./bin/clang++ -shared ${ldflags} -o ${output_dir}/${target_output_name}${output_extension} ${in} ${libs} ${frameworks}
  description = SOLINK ${output_dir}/${target_output_name}${output_extension}
rule stage2_unix_solink_module
  command = ./bin/clang++ -shared ${ldflags} -Wl,-flat_namespace -Wl,-undefined,suppress -o ${output_dir}/${target_output_name}${output_extension} ${in} ${libs} ${frameworks}
  description = SOLINK ${output_dir}/${target_output_name}${output_extension}
rule stage2_unix_link
  command = ./bin/clang++ ${ldflags} -o ${output_dir}/${target_output_name}${output_extension} ${in} ${libs} ${frameworks}
  description = LINK ${output_dir}/${target_output_name}${output_extension}
rule stage2_unix_stamp
  command = touch ${out}
  description = STAMP ${out}
rule stage2_unix_copy_bundle_data
  command = rm -rf ${out} && if [[ -d ${in} ]]; then cd ${in} && find . | cpio -pdl "$$OLDPWD"/${out} 2>/dev/null; else ln -f ${in} ${out} 2>/dev/null || (rm -rf ${out} && cp -af ${in} ${out}); fi
  description = COPY_BUNDLE_DATA ${in} ${out}
rule stage2_unix_compile_xcassets
  command = false
  description = The LLVM build doesn't use any xcasset files
rule stage2_unix_cc
  command = ./bin/clang -MMD -MF ${out}.d -o ${out} -c ${in} ${defines} ${include_dirs} ${cflags} ${cflags_c}
  description = CC ${out}
  depfile = ${out}.d
  deps = gcc
rule stage2_unix_cxx
  command = ./bin/clang++ -MMD -MF ${out}.d -o ${out} -c ${in} ${defines} ${include_dirs} ${cflags} ${cflags_cc}
  description = CXX ${out}
  depfile = ${out}.d
  deps = gcc

