  source_name_part = WindowsManifestMerger

build ./lib/libLLVMWindowsManifest.a: alink obj/llvm/lib/WindowsManifest/WindowsManifest.WindowsManifestMerger.o || obj/llvm/include/llvm/Config/config.stamp ./lib/libLLVMSupport.a obj/llvm/utils/gn/build/libs/xml/xml.stamp ./lib/libLLVMSupportBlake3.a ./lib/libLLVMDemangle.a
