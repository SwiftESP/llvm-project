defines =
include_dirs = -I../../llvm/include -Igen/llvm/include
cflags = -mmacos-version-min=10.10 -O3 -fdiagnostics-color -Wall -Wextra -Wno-unused-parameter -Wdelete-non-virtual-dtor -Wstring-conversion -no-canonical-prefixes -Werror=date-time -fPIC -Wcovered-switch-default
cflags_cc = -std=c++17 -fvisibility-inlines-hidden -fno-exceptions -fno-rtti
label_name = OpenACC
target_output_name = libLLVMFrontendOpenACC

build obj/llvm/lib/Frontend/OpenACC/OpenACC.ACC.o: cxx ../../llvm/lib/Frontend/OpenACC/ACC.cpp || obj/llvm/include/llvm/Config/AsmParsers.def.stamp obj/llvm/include/llvm/Config/AsmPrinters.def.stamp obj/llvm/include/llvm/Config/Disassemblers.def.stamp obj/llvm/include/llvm/Config/TargetExegesis.def.stamp obj/llvm/include/llvm/Config/TargetMCAs.def.stamp obj/llvm/include/llvm/Config/Targets.def.stamp obj/llvm/include/llvm/Config/abi-breaking.stamp obj/llvm/include/llvm/Config/config.stamp obj/llvm/include/llvm/Config/llvm-config.stamp obj/llvm/include/llvm/Frontend/OpenACC/ACC.stamp obj/llvm/include/llvm/Frontend/OpenACC/ACCcpp.stamp obj/llvm/include/llvm/Support/write_extension_def.stamp
  source_file_part = ACC.cpp
  source_name_part = ACC

build ./lib/libLLVMFrontendOpenACC.a: alink obj/llvm/lib/Frontend/OpenACC/OpenACC.ACC.o || obj/llvm/include/llvm/Frontend/OpenACC/acc_gen.stamp ./lib/libLLVMSupport.a ./lib/libLLVMSupportBlake3.a ./lib/libLLVMDemangle.a
  arflags =
  output_extension = 
  output_dir = ./lib
