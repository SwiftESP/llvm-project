defines =
include_dirs = -I../../clang-tools-extra/pseudo/include -I../../llvm/include -Igen/llvm/include -I../../clang/include -Igen/clang/include -Igen/clang-tools-extra/pseudo/include
cflags = -mmacos-version-min=10.10 -O3 -fdiagnostics-color -Wall -Wextra -Wno-unused-parameter -Wdelete-non-virtual-dtor -Wstring-conversion -no-canonical-prefixes -Werror=date-time -fPIC -Wcovered-switch-default -fno-strict-aliasing
cflags_cc = -std=c++17 -fvisibility-inlines-hidden -fno-exceptions -fno-rtti
label_name = cxx
target_output_name = libclangPseudoCXX

build obj/clang-tools-extra/pseudo/lib/cxx/cxx.CXX.o: cxx ../../clang-tools-extra/pseudo/lib/cxx/CXX.cpp || obj/clang-tools-extra/pseudo/include/CXXBNF.inc.stamp obj/clang-tools-extra/pseudo/include/CXXSymbols.inc.stamp obj/llvm/include/llvm/Config/AsmParsers.def.stamp obj/llvm/include/llvm/Config/AsmPrinters.def.stamp obj/llvm/include/llvm/Config/Disassemblers.def.stamp obj/llvm/include/llvm/Config/TargetExegesis.def.stamp obj/llvm/include/llvm/Config/TargetMCAs.def.stamp obj/llvm/include/llvm/Config/Targets.def.stamp obj/llvm/include/llvm/Config/abi-breaking.stamp obj/llvm/include/llvm/Config/config.stamp obj/llvm/include/llvm/Config/llvm-config.stamp obj/llvm/include/llvm/Support/write_extension_def.stamp
  source_file_part = CXX.cpp
  source_name_part = CXX

build ./lib/libclangPseudoCXX.a: alink obj/clang-tools-extra/pseudo/lib/cxx/cxx.CXX.o || obj/clang-tools-extra/pseudo/include/cxx_gen.stamp ./lib/libclangPseudoGrammar.a ./lib/libLLVMSupport.a ./lib/libLLVMSupportBlake3.a ./lib/libLLVMDemangle.a
  arflags =
  output_extension = 
  output_dir = ./lib
